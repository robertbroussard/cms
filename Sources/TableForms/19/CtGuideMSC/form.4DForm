				"Text1": {
					"type": "text",
					"top": 116,
					"left": 27,
					"width": 490,
					"height": 28,
					"stroke": "#000000",
					"fontFamily": "Arial",
					"fontSize": 10,
					"text": "Your service as a moderator is appreciated!  The following guidelines are provided for your convenience in conducting the  MSC."
				},
				"Text3": {
					"type": "text",
					"top": 150,
					"left": 27,
					"width": 507,
					"height": 28,
					"stroke": "#000000",
					"fontFamily": "Arial",
					"fontSize": 10,
					"text": "PURPOSE OF MSC:  To facilitate settlement by providing the parties and their attorneys with a confidential and professional evaluations of the strengths and weaknesses of the case."
				},
				"Text4": {
					"type": "text",
					"top": 191,
					"left": 125,
					"width": 9,
					"height": 17,
					"stroke": "#000000",
